{"ast":null,"code":"import MyButton from \"@/components/UI/MyButton\";\nimport MyInput from \"@/components/UI/MyInput\";\nimport { useFetchSub, useSubmit } from \"@/components/hooks/useSubmit\";\nimport { onMounted, ref } from \"vue\";\nconst required = val => !!val;\nconst minLength = num => val => val.length >= num;\nexport default {\n  components: {\n    MyInput,\n    MyButton\n  },\n  data() {\n    return {\n      changeInput: false\n    };\n  },\n  setup() {\n    const file = ref('');\n    const form = useSubmit({\n      name: {\n        value: ''\n      },\n      email: {\n        value: ''\n      },\n      tel: {\n        value: '',\n        validators: {\n          required,\n          minLength: minLength(8)\n        }\n      },\n      file: {\n        value: file.value\n      }\n    });\n    onMounted();\n    function handleFileUpload() {\n      file.value = this.$refs.file.files[0];\n    }\n    const submit = () => {\n      useFetchSub(form).then(i => console.log(i));\n    };\n    return {\n      form,\n      submit,\n      handleFileUpload\n    };\n  },\n  // methods:{\n  //   onFileChange(event) {\n  //     let files = event.target.files || event.dataTransfer.files;\n  //     if (files.length) this.changeInput = true;\n  //     else this.changeInput = true;\n  //   },\n  //   sendReq(){\n  //     console.log(this.name)\n  //     const file = this.$refs.file.files[0];\n  //     const formData = new FormData()\n  //     formData.append('name', 'sfdfsd')\n  //     formData.append('email', 'fsdf')\n  //     formData.append('tel', 'fsdfsdf')\n  //     formData.append('file', file)\n  //     axios.post('http://localhost:5000/emailer', formData, {\n  //       headers: {'Content-Type': 'multipart/form-data'}\n  //     })\n  //     alert('Отправлено')\n  //   }\n  // },\n  computed: {\n    view() {\n      if (this.changeInput) return {\n        display: 'inline-block'\n      };else return {\n        display: 'none'\n      };\n    },\n    noview() {\n      if (this.changeInput) return {\n        display: 'none'\n      };else return {\n        display: 'inline-block'\n      };\n    },\n    green() {\n      if (this.changeInput) return {\n        'border-color': 'rgba(0, 255, 0, 0.4)',\n        'background-color': 'rgba(0, 255, 0, 0.3)'\n      };\n    }\n  }\n};","map":{"version":3,"names":["MyButton","MyInput","useFetchSub","useSubmit","onMounted","ref","required","val","minLength","num","length","components","data","changeInput","setup","file","form","name","value","email","tel","validators","handleFileUpload","$refs","files","submit","then","i","console","log","computed","view","display","noview","green"],"sources":["C:\\Users\\ruste\\WebstormProjects\\camion-express-front\\src\\components\\MainPage\\SubmitApplication.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <pre>{{form}}</pre>\r\n    <h2>Оставить заявку</h2>\r\n    <div class=\"card\" @submit.prevent=\"submit\">\r\n      <my-input\r\n          type=\"text\"\r\n          v-model=\"form.name.value\"\r\n          placeholder=\"Имя\"\r\n          class=\"input\"\r\n      />\r\n      <my-input\r\n          type=\"email\"\r\n          v-model=\"form.email.value\"\r\n          placeholder=\"Email\"\r\n          class=\"input\"\r\n      />\r\n      <my-input\r\n          type=\"tel\"\r\n          v-model=\"form.tel.value\"\r\n          placeholder=\"Телефон\"\r\n          class=\"input\"\r\n          :class=\"{invalid: !form.tel.valid && form.tel.touched}\"\r\n          @blur=\"form.tel.blur\"\r\n      />\r\n      <small v-if=\"form.tel.touched && form.tel.errors.required\" style=\"color:#ff7b47; margin-left: 10px\">Введите номер телефона</small>\r\n      <small v-else-if=\"form.tel.touched && form.tel.errors.minLength\" style=\"color:#ff7b47; margin-left: 10px\">Недостаточное кол-во символов 8. Символов сейчас {{form.tel.value.length}}</small>\r\n      <div class=\"upload\">\r\n          <input\r\n              class=\"input__file\"\r\n              type=\"file\"\r\n              ref=\"file\"\r\n              @change=\"handleFileUpload\"\r\n              multiple\r\n          >\r\n        <div class=\"file-dummy\" :style=\"green\">\r\n          <div class=\"success\" :style=\"view\">Файл выбран</div>\r\n          <div class=\"default\" :style=\"noview\" >Выберите файл</div>\r\n        </div>\r\n\r\n        <router-link to=\"https://camionexpress.ru/\" class=\"sub\">Скачать файл</router-link>\r\n      </div>\r\n\r\n      <my-button :color=\"'orange'\" class=\"btn\" type=\"submit\" @click=\"submit\" :disabled=\"!form.valid\">Отправить</my-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MyButton from \"@/components/UI/MyButton\";\r\nimport MyInput from \"@/components/UI/MyInput\";\r\nimport {useFetchSub, useSubmit} from \"@/components/hooks/useSubmit\";\r\nimport {onMounted, ref} from \"vue\";\r\nconst required = val => !!val\r\nconst minLength = num => val => val.length >= num\r\nexport default {\r\n  components: {MyInput, MyButton},\r\n  data(){\r\n    return{\r\n      changeInput: false,\r\n    }\r\n  },\r\n  setup(){\r\n    const file = ref('')\r\n    const form = useSubmit({\r\n      name: {\r\n        value: '',\r\n      },\r\n      email:{\r\n        value : '',\r\n      },\r\n      tel:{\r\n        value: '',\r\n        validators: {required, minLength: minLength(8)}\r\n      },\r\n      file:{\r\n        value: file.value\r\n      }\r\n    });\r\n    onMounted()\r\n    function handleFileUpload(){\r\n      file.value = this.$refs.file.files[0];\r\n    }\r\n    const submit = () => {\r\n      useFetchSub(form).then(i => console.log(i))\r\n\r\n    }\r\n    return {form, submit, handleFileUpload}\r\n  },\r\n  // methods:{\r\n  //   onFileChange(event) {\r\n  //     let files = event.target.files || event.dataTransfer.files;\r\n  //     if (files.length) this.changeInput = true;\r\n  //     else this.changeInput = true;\r\n  //   },\r\n  //   sendReq(){\r\n  //     console.log(this.name)\r\n  //     const file = this.$refs.file.files[0];\r\n  //     const formData = new FormData()\r\n  //     formData.append('name', 'sfdfsd')\r\n  //     formData.append('email', 'fsdf')\r\n  //     formData.append('tel', 'fsdfsdf')\r\n  //     formData.append('file', file)\r\n  //     axios.post('http://localhost:5000/emailer', formData, {\r\n  //       headers: {'Content-Type': 'multipart/form-data'}\r\n  //     })\r\n  //     alert('Отправлено')\r\n  //   }\r\n  // },\r\n  computed: {\r\n    view(){\r\n      if(this.changeInput) return {display: 'inline-block'}\r\n      else return {display: 'none'}\r\n    },\r\n    noview(){\r\n      if(this.changeInput) return {display: 'none'}\r\n      else return {display: 'inline-block'}\r\n    },\r\n    green(){\r\n      if(this.changeInput) return {\r\n        'border-color' : 'rgba(0, 255, 0, 0.4)',\r\n        'background-color': 'rgba(0, 255, 0, 0.3)',\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n@import \"../../variables\";\r\n\r\nh2{\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.card{\r\n  @include card(white);\r\n  padding: 30px 50px;\r\n}\r\n.upload{\r\n  display: flex;\r\n  flex-direction: row;\r\n  width: 100%;\r\n  align-items: center;\r\n}\r\n.file-area {\r\n  width: 100%;\r\n  position: relative;\r\n}\r\n\r\n.file-dummy {\r\n  margin-top: 5px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: 100%;\r\n  min-height: 58px;\r\n  background: rgba(255, 255, 255, 0.2);\r\n  border: 2px dashed #252b42;\r\n  border-radius: 37px;\r\n  text-align: center;\r\n  transition: background 0.3s ease-in-out;\r\n}\r\n\r\n.file-dummy:hover {\r\n  background: rgba(255, 255, 255, 0.1);\r\n}\r\n.input__file{\r\n  position: absolute;\r\n  width: 50%;\r\n  opacity: 0;\r\n  cursor: pointer;\r\n}\r\n.input__file:focus + .file-dummy {\r\n  outline: 2px solid rgba(255, 255, 255, 0.5);\r\n  outline: -webkit-focus-ring-color auto 5px;\r\n}\r\n.input{\r\n  margin: 5px 0;\r\n}\r\n.invalid{\r\n  border-color: $c_orange;\r\n}\r\n.sub{\r\n  display: flex;\r\n  width: 100%;\r\n  margin-left: 1%;\r\n  min-height: 58px;\r\n  color: #252B42;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background: rgba(255, 255, 255, 0.2);\r\n  border: 2px dashed #252b42;\r\n  border-radius: 37px;\r\n  text-align: center;\r\n  transition: background 0.3s ease-in-out;\r\n  text-decoration: none;\r\n}\r\n\r\n.btn{\r\n  width: 100%;\r\n  height: 58px;\r\n  &:hover{\r\n    border-color: $c_orange;\r\n  }\r\n\r\n}\r\n</style>"],"mappings":"AAiDA,OAAOA,QAAO,MAAO,0BAA0B;AAC/C,OAAOC,OAAM,MAAO,yBAAyB;AAC7C,SAAQC,WAAW,EAAEC,SAAS,QAAO,8BAA8B;AACnE,SAAQC,SAAS,EAAEC,GAAG,QAAO,KAAK;AAClC,MAAMC,QAAO,GAAIC,GAAE,IAAK,CAAC,CAACA,GAAE;AAC5B,MAAMC,SAAQ,GAAIC,GAAE,IAAKF,GAAE,IAAKA,GAAG,CAACG,MAAK,IAAKD,GAAE;AAChD,eAAe;EACbE,UAAU,EAAE;IAACV,OAAO;IAAED;EAAQ,CAAC;EAC/BY,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,KAAKA,CAAA,EAAE;IACL,MAAMC,IAAG,GAAIV,GAAG,CAAC,EAAE;IACnB,MAAMW,IAAG,GAAIb,SAAS,CAAC;MACrBc,IAAI,EAAE;QACJC,KAAK,EAAE;MACT,CAAC;MACDC,KAAK,EAAC;QACJD,KAAI,EAAI;MACV,CAAC;MACDE,GAAG,EAAC;QACFF,KAAK,EAAE,EAAE;QACTG,UAAU,EAAE;UAACf,QAAQ;UAAEE,SAAS,EAAEA,SAAS,CAAC,CAAC;QAAC;MAChD,CAAC;MACDO,IAAI,EAAC;QACHG,KAAK,EAAEH,IAAI,CAACG;MACd;IACF,CAAC,CAAC;IACFd,SAAS,CAAC;IACV,SAASkB,gBAAgBA,CAAA,EAAE;MACzBP,IAAI,CAACG,KAAI,GAAI,IAAI,CAACK,KAAK,CAACR,IAAI,CAACS,KAAK,CAAC,CAAC,CAAC;IACvC;IACA,MAAMC,MAAK,GAAIA,CAAA,KAAM;MACnBvB,WAAW,CAACc,IAAI,CAAC,CAACU,IAAI,CAACC,CAAA,IAAKC,OAAO,CAACC,GAAG,CAACF,CAAC,CAAC;IAE5C;IACA,OAAO;MAACX,IAAI;MAAES,MAAM;MAAEH;IAAgB;EACxC,CAAC;EACD;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACAQ,QAAQ,EAAE;IACRC,IAAIA,CAAA,EAAE;MACJ,IAAG,IAAI,CAAClB,WAAW,EAAE,OAAO;QAACmB,OAAO,EAAE;MAAc,OAC/C,OAAO;QAACA,OAAO,EAAE;MAAM;IAC9B,CAAC;IACDC,MAAMA,CAAA,EAAE;MACN,IAAG,IAAI,CAACpB,WAAW,EAAE,OAAO;QAACmB,OAAO,EAAE;MAAM,OACvC,OAAO;QAACA,OAAO,EAAE;MAAc;IACtC,CAAC;IACDE,KAAKA,CAAA,EAAE;MACL,IAAG,IAAI,CAAC